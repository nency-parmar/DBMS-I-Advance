-- PART - B
CREATE VIEW VW_STU_INFO 
AS SELECT * FROM STUDENT_INFO WHERE SPI>8.5;

SELECT * FROM VW_STU_INFO;

CREATE VIEW VW_WITHOUT_BKLOG
AS SELECT * FROM STUDENT_INFO WHERE BKLOG=0;

SELECT * FROM VW_WITHOUT_BKLOG;

CREATE VIEW VW_COMPUTERVIEW 
AS SELECT * FROM STUDENT_INFO WHERE BRANCH = 'CE';

SELECT * FROM VW_COMPUTERVIEW;

-- PART - C
CREATE VIEW VW_RESULT_EC
AS SELECT FNAME,SPI FROM STUDENT_INFO WHERE SPI<5 AND BRANCH='EC';

SELECT * FROM VW_RESULT_EC;
UPDATE VW_RESULT_EC SET SPI=4.90 WHERE FNAME='MAHESH';

CREATE VIEW VW_STU_BKLOG
AS SELECT RNO,FNAME,BKLOG FROM STUDENT_INFO WHERE FNAME LIKE 'M____';

SELECT * FROM VW_STU_BKLOG;

DROP VIEW VW_COMPUTERVIEW;